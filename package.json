{
  "name": "debian-node-deploy",
  "version": "1.3.6",
  "description": "Unifying deployment tasks for my production server",
  "main": "index.js",
  "bin": {
    "deployNodeApp": "./cli/deploy.node.app.js",
    "deploySinglePageApp": "./cli/deploy.spa.app.js"
  },
  "scripts": {
    "clearjestcache": "jest --clearCache",
    "test": "jest --watchAll src --coverage",
    "build": "rimraf dist-deploy && tsc --project tsconfig-publish.json && node ./project-utils/scripts/copy.package.json.file.js",
    "start": "npm run build && node ./dist-deploy/index.js",
    "npmpublish": "npm run build && cd dist-deploy && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpb06/debian-node-deploy.git"
  },
  "author": "jpb06",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jpb06/debian-node-deploy/issues"
  },
  "homepage": "https://github.com/jpb06/debian-node-deploy#readme",
  "dependencies": {
    "ajv": "^6.12.2",
    "archiver": "^4.0.1",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "node-ssh": "^10.0.2",
    "ora": "^4.0.4"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/archiver": "^3.1.0",
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^25.2.3",
    "@types/node-ssh": "^7.0.0",
    "@types/ora": "^3.2.0",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "types": "./typings/index.d.ts"
}